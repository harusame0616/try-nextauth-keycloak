# .env.local にリネームしてください

NEXTAUTH_URL=http://localhost:13000

# openssl rand -base64 32 でシークレットを生成して設定
NEXTAUTH_SECRET={YOUR_SECRET}

# keycloak でクライアントを作成して値を設定する
KEYCLOAK_CLIENT_ID={YOUR_KEYCLOAK_CLIENT_ID}
KEYCLOAK_CLIENT_SECRET={YOUR_KEYCLOAK_CLIENT_SECRET}

# 対象の REALMS を設定する(デフォルトで master が存在)
KEYCLOAK_ISSUER=http://localhost:18080/realms/{YOUR_REALMS}
